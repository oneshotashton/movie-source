    <script type="text/javascript">
        $(document).ready(function(){
                    
        
            $(document).on('click', '#id_source a', function() {
               $("#id_source a").removeClass("active");
               $(this).addClass("active");
            });
           
            $('#iframe_code textarea').focus(function() {
                this.select();
            }).mouseup(function() {
                return false;
            });
            
            $(document).on('click', '#pills-tab button', function() {
                $("#iframe_test").removeAttr("style");
                
                var iframe_url = "https://vidsrc.me/embed/tt1300854/";
                var tab_id = $(this).attr("id");
                var tab_content_id = tab_id.replace("-tab","");
                
                var tab_content = $("#"+tab_content_id);
                tab_content.find("input").val("");
                tab_content.find("#iframe_code textarea").fadeOut("fast");
                tab_content.find("#iframe_code textarea").val("");
                
                if(tab_id == "pills-series-tab"){
                    iframe_url = "https://vidsrc.me/embed/tt0944947/2-3/";
                }
                $("#iframe_test iframe").attr("src" , iframe_url);
                
                
                
            });
            
            $(document).on('click', '#captcha_redo', function() {
                resetCaptcha();
            });
            
            $(document).on('click', '#request_button', function() {
                
                if($("#req_input").val().length === 0){
                    $("#req_input").focus();
                }else if($("#captcha_input").val().length === 0){
                    $("#captcha_input").focus();
                }else{
                    $.post("/request_submit.php",
                        {
                            url: $("#req_input").val(),
                            captcha_code: $("#captcha_input").val()
                        },
                        function(data_raw,status){
                            var data = JSON.parse(data_raw);
                            if(data.error){
                                $("#result p").html(data.error);
                                $("#result p").attr("class","").addClass("text-danger");
                            }else{
                                resetCaptcha();
                                $("#result p").html(data.msg);
                                $("#result p").attr("class","").addClass("text-success");
                                if(typeof data.embed_url !== 'undefined'){
                                   $("#result p").append(`
                                   <br/>
                                   <a href="`+data.embed_url+`" target="_blank">`+data.embed_url+`</a>
                                   `)
                                }
                                   
                            }
                        }
                    );
                }
                
            });
            
           
            
            $(".tab-pane").on("input", function() {
                
                
                var iframe_code = '<iframe src="URL" style="width: 100%; height: 100%;" frameborder="0" referrerpolicy="origin" allowfullscreen></iframe>';
                var parent = $(this);
                
                
                var id = parent.find("#source_id").val();
                var season = parent.find("#season").val();
                var episode = parent.find("#episode").val();
                
                var match_imdb = id.match(/^tt[0-9]+$/g);
                var match_tmdb = id.match(/^[0-9]+$/g);
                if(match_imdb == null && match_tmdb == null){
                    return false
                }
                
                if(season === undefined){
                    season = 0;
                }
                if(episode === undefined){
                    episode = 0;
                }
                
                
                
                var se_test = true;
                if(
                    $(this).attr("id") == "pills-series" &&
                    (season == 0 && episode == 0) &&
                    (season != 0 || episode != 0)
                ){
                    se_test = false;
                }
                
                
                
                
                
                if($(this).attr("id") == "pills-series"){
                    var type = "tv";
                }else{
                    var type = "movie";
                }
                
                
                if(se_test){
                    var iframe_url = "https://vidsrc.me/embed/"+type+"?";
                    if(match_imdb){
                        iframe_url += "imdb="+id;
                    }else if(match_tmdb){
                        iframe_url += "tmdb="+id;
                    }else{
                        return false;
                    }
                    
                    if(type == "tv" && season > 0 && episode > 0){
                        iframe_url += "&season="+season+"&episode="+episode
                    }
                    
                    iframe_code = iframe_code.replace("URL" , iframe_url);
                    
                    
                    $("#iframe_test iframe").attr("src" , iframe_url);
                    
                    parent.find("#iframe_code textarea").val(iframe_code);
                    parent.find("#iframe_code textarea").fadeIn("fast");
                    
                }else{
                    parent.find("#iframe_code textarea").fadeOut("fast");
                    parent.find("#iframe_code textarea").val("");
                }
            });
            
            
            
            
            function htmlEntities(str){
                return str.replace(/[\u00A0-\u9999<>\&]/gim, function(i) {
                    return '&#' + i.charCodeAt(0) + ';';
                });
            }
            
            
            function resetCaptcha(){
                $("#captcha_img").attr("src" , "").attr("src","/captcha/get_captcha.php?_="+Date.now());
            }
        
        
        });
